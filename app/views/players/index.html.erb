<h1>Listing players</h1>

<table>
  <tr>
    <th>User</th>
    <th>Alias</th>
    <th>Tournament</th>
  </tr>

<% for player in @players %>
  <tr>
    <td><%=h player.user.username %></td>
    <td><%=h player.alias %></td>
    <td><%=h player.tournament.title %></td>
  </tr>
<% end %>
</table>

<br />


