<h1>Listing players</h1>

<table>
  <tr>
    <th>Käyttäjä</th>
    <th>Alias</th>
    <th>Kohdesivu</th>
  </tr>

<% for player in @players %>
  <tr>
    <td><%=h player.user.username %></td>
    <td><%=h player.alias %></td>
    <td><li><%= link_to player.with_real_name, player_path(player) %></li>
</td>
  </tr>
<% end %>

</table>

<br />


