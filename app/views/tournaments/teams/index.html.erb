<h1>Listing teams</h1>

<table>
  <tr>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= link_to 'Show', tournament_team_path(@tournament, team) %></td>
    <td><%= link_to 'Edit', edit_tournament_team_path(@tournament, team) %></td>
    <td><%= link_to 'Destroy', tournament_team_path(@tournament, team), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team', new_tournament_team_path(@tournament) %>
